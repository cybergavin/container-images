        },
        {
          "created": "2025-07-16T06:08:39Z",
          "created_by": "RUN /bin/sh -c groupadd -g 1001 appgroup \u0026\u0026     useradd -m -u 1001 -g appgroup -s /bin/bash appuser \u0026\u0026     mkdir -p /app \u0026\u0026     chown appuser:appgroup /app # buildkit",
