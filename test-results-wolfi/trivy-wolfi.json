        },
        {
          "created": "2025-07-16T06:05:34Z",
          "created_by": "RUN /bin/sh -c apk update \u0026\u0026 apk upgrade \u0026\u0026 apk add --no-cache     curl     openssl     bash     jq     bind-tools     tzdata # buildkit",
          "comment": "buildkit.dockerfile.v0"
        },
        {
          "created": "2025-07-16T06:05:34Z",
          "created_by": "RUN /bin/sh -c addgroup -g 1001 appgroup \u0026\u0026     adduser -D -u 1001 -G appgroup -s /bin/bash appuser \u0026\u0026     mkdir -p /app \u0026\u0026     chown appuser:appgroup /app # buildkit",
          "comment": "buildkit.dockerfile.v0"
        },
        {
          "created": "2025-07-16T06:05:34Z",
          "created_by": "RUN /bin/sh -c cp /sbin/apk /sbin/apk.real # buildkit",
          "comment": "buildkit.dockerfile.v0"
        },
        {
          "created": "2025-07-16T06:05:34Z",
          "created_by": "RUN /bin/sh -c cat \u003e /sbin/apk \u003c\u003c'EOF'\n#!/bin/bash\n# Check for blocked commands\nfor arg in \"$@\"; do\n    case \"$arg\" in\n        upgrade)\n            echo \"ERROR: 'apk $arg' is not allowed. Use 'apk add' to install new packages.\"\n            echo \"Contact the Cloud Platform Services team if you need package updates.\"\n            exit 1\n            ;;\n    esac\ndone\nexec /sbin/apk.real \"$@\"\nEOF # buildkit",
          "comment": "buildkit.dockerfile.v0"
        },
        {
          "created": "2025-07-16T06:05:34Z",
          "created_by": "WORKDIR /app",
          "comment": "buildkit.dockerfile.v0"
        },
